    "@nuxtjs/storybook": "^4.2.0",
    "@nuxtjs/tailwindcss": "4.2.1",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@types/bcryptjs": "^2.4.2",
