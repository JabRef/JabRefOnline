    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^8.1.1",
    "jest": "^27.4.3",
    "jest-mock-extended": "^2.0.4",
    "nuxt-vite": "^0.3.5",
    "prettier": "^2.5.0",
    "prisma": "3.6.0",
    "redis-mock": "^0.56.3",
    "ts-jest": "^27.0.7"
  },
  "workspaces": {
    "nohoist": [
      "**/tailwindcss",
      "**/@tailwindcss/**"
    ]
  },
  "prisma": {
    "schema": "api/database/schema.prisma",
    "seed": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json ts-node ./api/database/runSeed.ts"
  }
}
