    "unstorage": "1.17.2",
    "uuid": "13.0.0",
    "vee-validate": "4.15.1",
    "vue": "3.5.22",
    "vue-router": "4.6.3",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@adonisjs/hash": "9.1.1",
    "@apollo/utils.keyvaluecache": "4.0.0",
    "@azure/core-rest-pipeline": "1.22.1",
    "@azure/static-web-apps-cli": "2.0.7",
