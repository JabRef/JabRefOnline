{
  "name": "jabref-online",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "yarn prisma:generate && yarn graphql:generate",
    "generate:watch": "concurrently \"yarn:*:generate:watch\"",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:create": "prisma migrate dev --create-only",
    "prisma:migrate:reset": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:resolve": "prisma migrate resolve",
    "prisma:push": "prisma db push",
    "prisma:seed": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json prisma db seed --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "graphql:generate": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json graphql-codegen",
    "graphql:generate:watch": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json graphql-codegen --watch",
    "graphql:validate": "graphql-inspector validate --apollo '{pages,components}/**/*.vue' 'api/**/*.graphql'",
    "docker:redis": "docker run -d --rm --name JabRefRedis -p 6380:6379 redis --requirepass jabref",
    "lint": "yarn lint:js && yarn lint:graphql",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:graphql": "cross-env TS_NODE_PROJECT=./tsconfig.cjs.json eslint --ext .graphql,.vue --ignore-path .gitignore .",
    "postinstall": "yarn generate",
    "test": "jest --runInBand",
    "test:integration": "jest integration.test.ts --runInBand",
    "test:unit": "jest spec.ts",
    "validate": "yarn graphql:validate"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
