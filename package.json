    "@vee-validate/zod": "^4.12.6",
    "@vue/apollo-composable": "4.0.2",
    "@vue/apollo-util": "^4.0.0-beta.6",
    "@vueuse/core": "^10.9.0",
    "@yaireo/tagify": "^4.24.0",
    "autoprefixer": "^10.4.17",
