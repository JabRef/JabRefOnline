{
  "name": "jabref-online",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi generate --fail-on-error",
    "start": "nuxi preview",
    "generate": "yarn prisma:generate && yarn graphql:generate",
    "generate:watch": "concurrently \"yarn:*:generate:watch\"",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:create": "prisma migrate dev --create-only",
    "prisma:migrate:diff": "prisma migrate diff --exit-code --from-migrations api/database/migrations --to-schema-datamodel api/database/schema.prisma --shadow-database-url",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:resolve": "prisma migrate resolve",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "storybook": "nuxt storybook",
    "storybook:build": "nuxt storybook build",
    "storybook@17": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxt storybook",
    "storybook:build@17": "cross-env NODE_OPTIONS=--openssl-legacy-provider nuxt storybook build",
    "storybook:publish": "chromatic --exit-zero-on-changes --build-script-name storybook:build",
    "graphql:generate": "graphql-codegen",
    "graphql:generate:watch": "graphql-codegen --watch",
    "graphql:validate": "graphql-inspector validate --apollo '{pages,components}/**/*.vue' 'api/**/*.graphql'",
    "docker:redis": "docker run -d --rm --name JabRefRedis -p 6380:6379 redis --requirepass jabref",
    "lint": "yarn lint:js && yarn lint:graphql",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:graphql": "eslint --ext .graphql,.vue --ignore-path .gitignore .",
    "postinstall": "patch-package && yarn generate && nuxi prepare",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest test.ts --runInBand",
    "test:unit": "cross-env NODE_OPTIONS=--experimental-vm-modules jest spec.ts",
    "typecheck": "nuxi typecheck",
    "validate": "yarn graphql:validate"
  },
  "dependencies": {
    "@apollo/client": "^3.6.7",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^2.0.6",
    "@graphql-tools/graphql-file-loader": "^7.3.15",
    "@graphql-tools/load": "^7.5.14",
    "@graphql-tools/merge": "^8.2.14",
    "@graphql-tools/schema": "^8.3.11",
    "@he-tree/vue2": "^1.3.10",
    "@pinia/nuxt": "^0.1.9",
    "@prisma/client": "^3.15.1",
    "@vue/apollo-composable": "4.0.0-alpha.17",
    "@vue/apollo-util": "^4.0.0-alpha.17",
    "@vueuse/core": "^8.6.0",
    "@yaireo/tagify": "^4.12.0",
    "apollo-server-express": "^3.8.2",
    "autoprefixer": "^10.4.7",
    "connect-redis": "^6.1.3",
    "cross-fetch": "^3.1.5",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier-vue": "^4.1.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "graphql": "^16.5.0",
    "graphql-passport": "^0.6.4",
    "graphql-scalars": "^1.17.0",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.5",
    "nuxt-edge": "latest",
    "nuxt-route-meta": "^2.3.4",
    "passport": "^0.6.0",
    "pinia": "^2.0.14",
    "portal-vue": "^2.1.7",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^8.3.0",
    "ts-node": "^10.8.1",
    "tsyringe": "^4.7.0",
    "typescript": "^4.7.3",
    "vue-apollo": "^3.1.0",
    "vue-tailwind": "^2.5.1",
    "vue-virtual-scroll-list": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/gql-tag-operations-preset": "^1.4.0",
    "@graphql-codegen/typed-document-node": "^2.2.13",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-resolvers": "^2.6.6",
    "@graphql-eslint/eslint-plugin": "^3.10.4",
    "@graphql-inspector/cli": "^3.1.2",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@nuxt/bridge": "npm:@nuxt/bridge-edge",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.0.6",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.1.0",
    "@nuxtjs/storybook": "^4.3.2",
    "@nuxtjs/tailwindcss": "5.1.2",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.182",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.8",
    "@types/redis-mock": "^0.17.1",
    "@types/uuid": "^8.3.4",
    "@types/yaireo__tagify": "^4.12.0",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/runtime-dom": "^3.2.37",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue2-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "chromatic": "^6.5.6",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^3.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "eslint-plugin-vue": "^9.1.1",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.6.2",
    "prisma": "3.15.1",
    "redis-mock": "^0.56.3",
    "ts-jest": "^28.0.4",
    "vue-tsc": "^0.34.16"
  },
  "resolutions": {
    "@graphql-tools/mock": "^8.4.3",
    "@types/react": "https://registry.yarnpkg.com/@favware/skip-dependency/-/skip-dependency-1.1.1.tgz",
    "jest": "^27.5.1"
  },
  "resolutionsComments": {
    "@graphql-tools/mock": "Can be removed once apollo-server updated its dependencies: https://github.com/apollographql/apollo-server/issues/5901",
    "@types/react": "Otherwise these types interfere with the types from vite: https://github.com/johnsoncodehk/volar/discussions/592#discussioncomment-1580518",
    "jest": "Workaround for https://github.com/hypervillain/ast-to-literal/issues/11 which pulls in old version of jest"
  },
  "workspaces": {
    "nohoist": [
      "**/tailwindcss",
      "**/@tailwindcss/**"
    ]
  },
  "prisma": {
    "schema": "api/database/schema.prisma",
    "seed": "node --experimental-specifier-resolution=node --loader ts-node/esm ./api/database/runSeed.ts"
  }
}
