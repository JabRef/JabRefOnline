    "babel-plugin-transform-typescript": "^7.0.0-alpha.19",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "connect-redis": "^6.0.0",
    "cross-fetch": "^3.1.4",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.7.2",
    "graphql-passport": "^0.6.4",
    "graphql-scalars": "^1.13.6",
    "lodash": "^4.17.21",
    "nodemailer": "^6.7.2",
    "nuxt": "^2.15.8",
    "nuxt-route-meta": "^2.3.2",
    "passport": "^0.5.0",
    "pinia": "^2.0.5",
