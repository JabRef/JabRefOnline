    "@vue/composition-api": "^1.0.0-rc.12",
    "@yaireo/tagify": "^4.3.1",
    "apollo-server-express": "^2.25.2",
    "autoprefixer": "^10.2.6",
    "babel-plugin-transform-import-meta": "^2.0.0",
