    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "unstorage": "^1.10.1",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.22",
    "vue-router": "^4.3.0",
    "zod": "^3.22.4"
  },
