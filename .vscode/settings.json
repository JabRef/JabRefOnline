{
  "cSpell.words": [
    "Biblatex",
    "Nuxt",
    "booktitle",
    "codegen",
    "datetime",
    "jiti",
    "journaltitle",
    "nuxtjs",
    "transpiled",
    "tsyringe",
    "upsert"
  ],
  "vetur.experimental.templateInterpolationService": true,
  "[jsonc]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
  },
  "files.exclude": {
    "node_modules/": true,
    ".nuxt": true,
    "coverage": true
  },
  "jest.jestCommandLine": "yarn test",
  "eslint.validate": [
    "javascript",
    "typescript",
    "graphql",
    "vue"
  ],
  "eslint.options": {
    "extentions": [
      ".js",
      ".graphql",
      ".vue",
    ]
  },
  "vscode-graphql.useSchemaFileDefinitions": true
}
